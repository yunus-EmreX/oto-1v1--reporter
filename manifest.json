{
  "manifest_version": 2,
  "name": "WebSocket Monitor",
  "version": "1.0",
  "description": "Intercept and send data to WebSocket connections",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
